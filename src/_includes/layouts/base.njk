<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/assets/css/reset.css' | url }}">
    <link rel="stylesheet" href="{{ '/assets/css/main.css' | url }}">
    <link rel="stylesheet" href="{{ '/assets/css/home.css' | url }}">
    <link rel="stylesheet" href="{{ '/assets/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
  </head>
  <body>
  <div class="main-header">
    <header class="navi">
      <ul class="nav">
        {%- for nav in collections.nav | reverse -%}
        <li class="nav-item{% if nav.url == page.url %} nav-item-active{% endif %}"><a href="{{ nav.url | url }}">{{ nav.data.navtitle }}</a></li>
        {%- endfor -%}
        <li class="nav-item"><a href="#">About</a></li>
        <li class="nav-item"><a href="#">Contribute</a></li>
        <li class="nav-item"><a href="#">Latest News</a></li>
      </ul>
    </header>
  </div>
    <main{% if templateClass %} class="{{ templateClass }} home"{% endif %}>
      {{ content | safe }}
    </main>

    <footer class="footer">
      <p><em>This is an <a href="https://www.11ty.io/">Eleventy project</a>.</em></p>
    </footer>

    
  </body>
</html>
